<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edge.coin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".CoinApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/WhiteTheme">
        <activity android:name=".MainPackage.MainActivity"></activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".ServicePackage.DataService" />

        <activity android:name=".RadarActivity"></activity>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="1507ebac14d23dc9042cfe521ca5720a96ccff03" />
    </application>

</manifest>